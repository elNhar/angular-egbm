{
    "name": "angular-egbm",
    "version": "0.0.0",
    "scripts": {
        "config": "ts-node ./src/assets/scripts/setEnv.ts",
        "ng": "ng",
        "start": "npm run config -- --environment=dev && ng serve --poll=2000",
        "build-dev": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "dev:ssr": "npm run config -- --environment=dev && ng run angular-egbm:serve-ssr",
        "serve:ssr": "node dist/angular-egbm/server/main.js",
        "build:ssr": "npm run config -- --environment=prod && ng build && ng run angular-egbm:server",
        "prerender": "ng run angular-egbm:prerender",
        "vercel-build": "npm run build:ssr"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.2.0",
        "@angular/cdk": "^15.2.0",
        "@angular/common": "^15.2.4",
        "@angular/compiler": "^15.2.0",
        "@angular/core": "^15.2.0",
        "@angular/forms": "^15.2.0",
        "@angular/platform-browser": "^15.2.0",
        "@angular/platform-browser-dynamic": "^15.2.0",
        "@angular/platform-server": "^15.2.0",
        "@angular/router": "^15.2.0",
        "@contentful/rich-text-html-renderer": "^16.0.3",
        "@contentful/rich-text-plain-text-renderer": "^16.0.3",
        "@emailjs/browser": "^3.10.0",
        "@ng-bootstrap/ng-bootstrap": "^14.0.1",
        "@ng-select/ng-select": "^10.0.4",
        "@nguniversal/express-engine": "^15.2.0",
        "@ngx-axios-adapter/core": "^1.0.6",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.3",
        "contentful": "^9.3.5",
        "contentful-management": "^10.38.0",
        "contentful-sitemap": "^1.0.20",
        "express": "^4.15.2",
        "jsonwebtoken": "^9.0.0",
        "mock-browser": "^0.92.14",
        "ng-angular-popup": "^0.2.0",
        "ng-recaptcha": "^11.0.0",
        "ngx-pagination": "^6.0.3",
        "ngx-slider-v2": "^15.0.4",
        "rxjs": "~7.8.0",
        "swiper": "^8.4.6",
        "ts-node": "^10.9.1",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.2.2",
        "@angular/cli": "~15.2.2",
        "@angular/compiler-cli": "^15.2.0",
        "@angular/localize": "^15.2.9",
        "@nguniversal/builders": "^15.2.0",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~4.3.0",
        "@types/leaflet": "^1.9.3",
        "@types/node": "^14.15.0",
        "dotenv": "^16.0.3",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "typescript": "~4.9.4",
        "yargs": "^17.7.1"
    }
}
